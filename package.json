{
  "name": "board-game-marketplace",
  "version": "1.0.0",
  "description": "A centralized web platform for discovering and reviewing board games",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "server": "nodemon backend/server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "install:all": "npm install && cd frontend && npm install",
    "seed": "node backend/seedData.js"
  },
  "keywords": ["board-games", "marketplace", "react", "nodejs", "express"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
}